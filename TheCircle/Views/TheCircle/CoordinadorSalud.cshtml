<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>TheCircle - Coordinador salud</title>

    @Html.Partial("Partials/Styles")

</head>
<body class="page-dashboard" ng-app="coordinacionSalud">
    <!-- TopBar -->
    @Html.Partial("Partials/TopBar")
    <!-- TopBar -->
    <div class="content-wrapper">

        @Html.Partial("Partials/SidebarStyles")

        <div class="sidebar" id="sidebar_menu">
            <ul class="sidebar-nav">
                <li class=" open" ui-sref="validar" ui-sref-active="active">
                    <a>
                        <span>Validar Remisiones</span>
                    </a>
                </li>
                <li class=" open" ui-sref="remisionesRechazadas" ui-sref-active="active">
                    <a>
                        <span>Remisiones Rechazadas</span>
                    </a>
                </li>
                <li class="open" ui-sref="remisionesAprobadas" ui-sref-active="active">
                    <a>
                        <span>Remisiones Aprobadas</span>
                    </a>
                </li>
                <li class=" open" ui-sref="stock" ui-sref-active="active">
                    <a>
                        <span>Ver Stock</span>
                    </a>
                </li>
                <li class=" open" ui-sref="transferencias" ui-sref-active="active">
                    <a>
                        <span>Transferencias</span>
                    </a>
                </li>
                <li class=" open" ui-sref="reporte" ui-sref-active="active">
                    <a>
                        <span>Reportes</span>
                    </a>
                </li>
                <!--
                <li class=" open" ui-sref="historial" ui-sref-active="active">
                    <a>
                        <span>Historial</span>
                    </a>
                </li>
                -->
            </ul>
        </div>

        <div style="overflow-y:auto;max-height:94vh">
            <ui-view></ui-view>
        </div>

    </div>

    @Html.Partial("Partials/Scripts")
    <script src="~/scripts/app/coordinacionSalud.js"></script>

</body>
</html>

