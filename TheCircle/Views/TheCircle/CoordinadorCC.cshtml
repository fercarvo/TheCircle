<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>TheCircle - Coordinador salud</title>

    @Html.Partial("Partials/Styles")

</head>
<body class="page-dashboard" ng-app="coordinadorCC">
    <!-- TopBar -->
    @Html.Partial("Partials/TopBar")
    <!-- TopBar -->
    <div class="content-wrapper">

        @Html.Partial("Partials/SidebarStyles")

        <div class="sidebar" id="sidebar_menu">
            <ul class="sidebar-nav">
                <li data-ripple class=" open" ui-sref="recetas" ui-sref-active="active">
                    <a>
                        <span>Ver Recetas</span>
                    </a>
                </li>
                <li data-ripple class=" open" ui-sref="egresos" ui-sref-active="active">
                    <a>
                        <span>Ver Egresos</span>
                    </a>
                </li>
            </ul>
        </div>

        <div style="overflow-y:auto;max-height:100vh;">
            <ui-view></ui-view>
        </div>

    </div>

    @Html.Partial("Partials/Scripts")
    <script src="~/scripts/app/coordinadorCC.js"></script>

</body>
</html>
