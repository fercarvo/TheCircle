<!DOCTYPE html>
<html lang="es">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>TheCircle - Contralor</title>

    @Html.Partial("Partials/Styles")

</head>
<body class="page-dashboard" ng-app="contralor">
    <!-- TopBar -->
    @Html.Partial("Partials/TopBar")
    <!-- TopBar -->
    <div class="content-wrapper">

        @Html.Partial("Partials/SidebarStyles")

        <div class="sidebar" id="sidebar_menu">
            <ul class="sidebar-nav">
                <li data-ripple class=" open" ui-sref="aprobar" ui-sref-active="active">
                    <a>
                        <span>Aprobar Remisiones</span>
                    </a>
                </li>
                <li data-ripple class=" open" ui-sref="inconsistencias" ui-sref-active="active">
                    <a>
                        <span>Inconsistencias</span>
                    </a>
                </li>
                <li data-ripple class="open" ui-sref="historial" ui-sref-active="active">
                    <a>
                        <span>Historial</span>
                    </a>
                </li>
                <li data-ripple class=" open" ui-sref="editarStock" ui-sref-active="active">
                    <a>
                        <span>Editar stock</span>
                    </a>
                </li>
            </ul>
        </div>

        <div style="overflow-y:no-display;max-height:94vh">
            <ui-view></ui-view>
        </div>

    </div>
    @Html.Partial("Partials/Scripts")
    <script src="~/scripts/app/contralor.js"></script>
</body>
</html>